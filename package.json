{
  "name": "qalclaude",
  "version": "0.2.0",
  "type": "module",
  "description": "Claude Code with qalcode's beautiful TUI - OpenTUI/SolidJS version",
  "bin": {
    "qalclaude": "./bin/qalclaude"
  },
  "scripts": {
    "start": "bun run src/main.tsx",
    "dev": "bun run --watch src/main.tsx",
    "build": "bun build src/main.tsx --outdir dist --target node",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@opentui/core": "0.1.59",
    "@opentui/solid": "0.1.59",
    "solid-js": "1.9.5",
    "opentui-spinner": "0.0.6",
    "fuzzysort": "3.1.0",
    "strip-ansi": "7.1.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "5.7.3"
  }
}
